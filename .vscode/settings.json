      "workbench.colorCustomizations"
    ],
    "biome.suggestInstallingGlobally": false,
    "editor.accessibilitySupport": "off",
    "editor.minimap.enabled": false,
        "editor.codeActionsOnSave": {

    "source.fixAll.biome": "explicit",
  },
  "workbench.editor.editorActionsLocation": "hidden",
  "gitlens.ai.model": "gitkraken",
  "gitlens.ai.gitkraken.model": "gemini:gemini-2.0-flash",
  "zig.zls.enabled": "on",
  "workbench.iconTheme": "catppuccin-macchiato",

  "go.inlayHints.functionTypeParameters": true,
  "go.inlayHints.ignoredError": true,
  "go.inlayHints.parameterNames": true,
  "workbench.colorTheme": "Sequoia Moonlight",
  "gitlens.advanced.messages": {
    "suppressIntegrationDisconnectedTooManyFailedRequestsWarning": true
  },
  "workbench.preferredDarkColorTheme": "Vesper",
  "chat.commandCenter.enabled": false,
  "chat.agentsControl.enabled": false
